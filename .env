OPENROUTER_MODEL=meta-llama/llama-3.3-70b-instruct

# 指定 provider（可选，留空则使用默认路由）
# 例如: OpenAI, Anthropic, Together, Fireworks 等
OPENROUTER_PROVIDER=cerebras/fp16

# API 调用最小间隔（秒）
OPENROUTER_MIN_INTERVAL=0.1

USE_LLM_TEXT_MUTATION=1
LLM_TEXT_MUTATION_PROB=0.8
LLM_TEXT_MAX_TRIES=3
LLM_TEXT_MAX_CHARS=650
USE_LLM_ICL_MUTATION=1
LLM_ICL_MUTATION_PROB=0.7
LLM_ICL_MAX_TRIES=3
